{
    "version": "1.2.0",
    "name": "nerdroid23/blade-icomoon",
    "description": "A package to easily make use of \"IcoMoon\" in your Laravel Blade views.",
    "keywords": [
        "laravel",
        "blade",
        "icomoon"
    ],
    "homepage": "https://github.com/nerdroid23/blade-icomoon",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Sylnice",
            "email": "nerdroid23@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "blade-ui-kit/blade-icons": "^1.5",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "liip/rmt": "^1.6",
        "nunomaduro/collision": "^5.3|^6.0|^7.0|^8.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nerdroid23\\BladeIcomoon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nerdroid23\\BladeIcomoon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nerdroid23\\BladeIcomoon\\BladeIcomoonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
